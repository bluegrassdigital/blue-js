{
  "name": "blue-js",
  "version": "1.0.5",
  "description": "A super lightweight functional commonjs dom & events library",
  "main": "main.js",
  "scripts": {
    "test": "mocha ./*/tests/*.js",
    "jsdoc": "jsdoc2md --template README.source.md */*.js > README.md",
    "standard": "standard ./*/*.js",
    "standardTests": "standard --env mocha ./*/tests/*.js",
    "start": "npm run standard && npm run standardTests && npm run test && npm run jsdoc"
  },
  "author": {
    "name": "Simon Buerger",
    "url": "https://twitter.com/simonbuerger"
  },
  "license": "MIT",
  "issues": "http://github.com/bluegrassdigital/blue-js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluegrassdigital/blue-js.git"
  },
  "keywords": [
    "dom",
    "library",
    "events"
  ],
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "mocha-jsdom": "^1.1.0",
    "standard": "^10.0.1"
  }
}
