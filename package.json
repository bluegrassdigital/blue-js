{
  "name": "blue-js",
  "version": "2.2.0",
  "description": "A super lightweight functional commonjs dom & events library",
  "files": [
    "es",
    "index.js",
    "dom",
    "events",
    "lang",
    "ps",
    "umd",
    "utils"
  ],
  "main": "index.js",
  "module": "es/index.js",
  "scripts": {
    "coverage": "nyc --reporter=text-lcov npm test > coverage.lcov",
    "build": "node ./tools/build.js",
    "watch": "babel ./modules -d . --ignore __tests__ --watch",
    "test": "mocha --require @babel/register ./modules/*/__tests__/*.js",
    "jsdoc": "jsdoc2md --template README.source.md modules/**/*.js > README.md",
    "standard": "standard ./modules/*.js",
    "standardTests": "standard --env mocha ./modules/*/__tests__/*.js",
    "prepublishOnly": "npm run build && npm run test && npm run jsdoc",
    "start": "npm run build && npm run standard && npm run standardTests && npm run test && npm run jsdoc"
  },
  "author": {
    "name": "Simon Buerger",
    "url": "https://twitter.com/simonbuerger"
  },
  "license": "MIT",
  "issues": "http://github.com/bluegrassdigital/blue-js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluegrassdigital/blue-js.git"
  },
  "keywords": [
    "dom",
    "library",
    "events"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.1.2",
    "gzip-size": "^5.0.0",
    "jsdom": "^12.0.0",
    "mocha": "^5.2.0",
    "mocha-jsdom": "^2.0.0",
    "nyc": "^13.0.1",
    "pretty-bytes": "^5.1.0",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^5.0.2",
    "standard": "^12.0.1"
  }
}
